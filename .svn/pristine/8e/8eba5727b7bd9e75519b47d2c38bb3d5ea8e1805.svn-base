/* End Mixins */
* {
  margin: 0;
  padding: 0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box; }

html {
  overflow: scroll; }

body {
  font-family: verdana;
  font-size: 12px; }

a {
  text-decoration: none; }

ul {
  list-style: none; }

/* End Global Styles */
/*------------------------------------------------------------------

    Misc Classes

-------------------------------------------------------------------*/
.clear {
  clear: both; }

.left {
  float: left; }

.right {
  float: right; }

.bold {
  font-weight: bold; }

.italic {
  font-style: italic; }

.underline {
  text-decoration: underline; }

.center_text {
  text-align: center; }

.all_caps {
  text-transform: uppercase; }

.lower_case {
  text-transform: lowercase; }

/* End Misc Classes */
